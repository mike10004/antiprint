{
  "manifest_version": 2,
  "name": "Sample Extension",
  "description": "For testing purposes",
  "version": "1.0",
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon-19.png",
    "default_title": "Sample Extension"
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["inject.js"],
    "run_at": "document_start"
  }]
}
